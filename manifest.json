{
	
	"manifest_version": 2,
	
	"default_locale": "zh_CN",

	"name": "__MSG_title__",
	
	"version": "1.0.0",
	
	"description": "__MSG_desc__",
	
	
	"browser_action": 
	{	
		"default_title": "popup deprecated!",
		"default_popup": "popup.html"
	},
	
	
	"content_scripts": 
	[
		{

			"matches": ["https://*.bilibili.com/*"],
			
			"js": ["injector.js"],
			
			"run_at": "document_end"
		}
	],
	
	"permissions":
	[
		"contextMenus", 
		"tabs", 
		"notifications", 
		"webRequest", 
		"webRequestBlocking",
		"storage"
	],
	"web_accessible_resources": [
        "*.js"
    ],
	"homepage_url": "https://127.0.0.1"

	
	
	
}